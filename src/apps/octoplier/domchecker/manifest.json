{
  "manifest_version": 2,

  "name": "Dom Checker",
  "description": "This extension provides some insight into the dom",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["utils.js", "content.js"],
      "css": [],
      //"all_frames": true,
      "run_at": "document_start"
    }
  ],

  "permissions": [
  	"<all_urls>",
    "activeTab",
    "browsingData",
    "nativeMessaging"
  ]
  

}
