{
	// "C:\Program Files (x86)\Google\Chrome\Application\chrome.exe" --disable-web-security --user-data-dir --app=https://www.google.com

  "manifest_version": 2,

  "name": "Dom Checker",
  "description": "This extension provides some insight into the dom",
  "version": "1.0",

  "icons": { "16": "images/octopus_blue_16.png",
           "48": "images/octopus_blue_48.png",
          "128": "images/octopus_blue_128.png" 
          },

  "background": {
      "scripts": ["background.js"]
    //"scripts": ["extensionserverport.js", "browserwrap.js","bgcomm.js", "bgcommhandler.js", "bgmain.js"]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      // "js": ["utils.js", 
      // 		 "point.js", 
      // 		 "box.js", 
      // 		 "contentcomm.js", 
      // 		 "contentcommhandler.js", 
      // 		 "matchcriteria.js",
      		  
      // 		 "elemwrap.js", 
      // 		 "pagewrap.js", 
      // 		 "popupdialog.js", 
      // 		 "distinctcolors.js",
      // 		 "overlay.js",  
      // 		 "overlayset.js",
      // 		 "overlaysets.js", 
      // 		 "eventblocker.js", 
      // 		 "guicollection.js", 
      		 
      // 		 "mutationmonitor.js",
      // 		 "contextmenu.js", 
      // 		 "contextmenuhandler.js",
      // 		 "contentmain.js"],
      "css": ["contextmenu.css", "overlay.css", "popupdialog.css"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  
  "web_accessible_resources": [
    "contextmenu.html",
    "images/hourglass.gif"
  ],

  "permissions": [
  	"cookies",
  	"http://*/*", 
  	"https://*/*",
  	"<all_urls>",
  	"contextMenus",
    "activeTab",
    "browsingData",
    "nativeMessaging",
    "history"
  ]
  

}
